doi = {10.1364/CLEO_QELS.2015.FF1A.5},
abstract = {We experimentally demonstrate quantum key distribution at 28 bit/s rate in a telecommunications fiber channel with 45 dB loss using a subcarrier wave approach. This approach offers polarization independency, high bitrates and wide multiplexing capabilities.},
}

