@article{Gleĭm:17,
author = {A. V. Gleĭm and V. V. Chistyakov and O. I. Bannik and V. I. Egorov and N. V. Buldakov and A. B. Vasilev and A. A. Gaĭdash and A. V. Kozubov and S. V. Smirnov and S. M. Kynev and S. \'{E}. Khoruzhnikov and S. A. Kozlov and V. N. Vasil'ev},
