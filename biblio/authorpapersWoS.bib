@article{Chistyakov_2014,
	doi = {10.1088/1742-6596/541/1/012078},
	url = {https://doi.org/10.1088%2F1742-6596%2F541%2F1%2F012078},
	year = 2014,
	month = {oct},
	publisher = {{IOP} Publishing},
	volume = {541},
	pages = {012078},
	author = {V V Chistyakov and A V Gleim and V I Egorov and Yu V Nazarov},
	title = {Implementation of multiplexing in a subcarrier-wave quantum cryptography system},
	journal = {Journal of Physics: Conference Series},
	abstract = {Quantum cryptography allows distributing secure keys in a way that any eavesdropping in the channel is inevitably detected. This work is dedicated to introducing wavelength division multiplexing in a subcarrier-wave quantum cryptography system. Compared to other existing schemes, the resulting device is able to achieve higher bitrates (up to 2.26 Mbit/s at 20 km), is robust against external conditions and compatible with standard telecommunication fibres in multi-user environment.}
}

